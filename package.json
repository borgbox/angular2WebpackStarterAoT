{
  "name": "angular2-starter",
  "version": "0.2.1",
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "npm run codegen && cross-env APP_ENVIRONMENT=production webpack -p --progress",
    "codegen": "ngc && rimraf src/*.js src/**/*.js",
    "postinstall": "typings install",
    "serve": "webpack-dev-server --inline --progress"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.1",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "angular2-template-loader": "0.5.0",
    "cross-env": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "ts-loader": "0.8.2",
    "typescript": "2.0.3",
    "typings": "1.4.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.5"
  }
}
